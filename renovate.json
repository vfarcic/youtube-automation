{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
      "config:recommended"
  ],
  "packageRules": [
    {
      "matchManagers": ["devbox"],
      "automerge": true,
      "commitMessageSuffix": "[skip ci]"
    },
    {
      "matchManagers": ["gomod"],
      "matchUpdateTypes": ["patch", "minor"],
      "automerge": true
    }
  ]
}
