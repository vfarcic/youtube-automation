{
  "mcpServers": {
    "dot-ai": {
      "command": "docker",
      "args": [
        "compose",
        "-f",
        "${PWD}/docker-compose-dot-ai.yaml",
        "run",
        "--rm",
        "--remove-orphans",
        "dot-ai"
      ]
    },
    "coderabbitai": {
      "command": "npx",
      "args": ["coderabbitai-mcp@latest"],
      "env": {
        "GITHUB_PAT": "${GITHUB_TOKEN}"
      }
    }
  },
  "general": {
    "previewFeatures": true
  }
}